    issn = {1939-1382},
    journal = {IEEE Transactions on Learning Technologies},
    keywords = {Generative AI;Education;Artificial intelligence;Learning (artificial intelligence);Guidelines;Task analysis;Transformers;Generative artificial intelligence;human-centered;learning and teaching framework;pedagogical design;self-regulated learning (SRL)},
    month = {},
    number = {},
    pages = {1562-1573},
    title = {{A} Human-{C}entered {L}earning and {T}eaching {F}ramework {U}sing {G}enerative {A}rtificial {I}ntelligence for {S}elf-{R}egulated {L}earning {D}evelopment {T}hrough {D}omain {K}nowledge {L}earning in {K}\textendash 12 {S}ettings},
    volume = {17},
    year = {2024}
}

@inproceedings{11158697,
    abstract = {This study conducts a systematic comparative analysis of artificial intelligence (AI) integration in primary music education between China and Korea. Utilizing a mixed-methods framework, the research examines policy frameworks, pedagogical implementations, and educator perceptions through policy document analysis, classroom observations, and semi-structured interviews with 20 music educators. Findings reveal that while both nations leverage AI to address challenges such as large class sizes and resource disparities, China emphasizes standardized AI platforms to enhance equity in rural education, whereas Korea prioritizes teacher-designed AI applications to foster urban innovation. Notably, Korean educators report a 34\% higher incidence of AI-facilitated collaborative composition activities compared to their Chinese counterparts (p<0.05). However, systemic barriers including uneven teacher training and technological infrastructure persist in rural regions. This study contributes to the global dialogue on AI in arts education by proposing a culturally responsive implementation framework that balances technological innovation with pedagogical sustainability.},
