    title = {{E}nvironmental {A}ssessment and {I}mprovement of {F}actory {B}uilding {D}esigns based on {G}enerative {A}rtificial {I}ntelligence},
    url = {https://www.sciencedirect.com/science/article/pii/S2212827125004184},
    volume = {135},
    year = {2025}
}

